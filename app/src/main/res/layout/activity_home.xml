            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/homeicon_border"
            android:layout_weight="1"
            android:layout_marginRight="7dp">
            <ImageView
                android:id="@+id/image_dagger"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_omanidagger"
                android:paddingTop="@dimen/margin40dp"/>
            <TextView
                android:id="@+id/sdagger_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/omani_dagger"
                android:gravity="center_horizontal"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
                android:padding="9dp"
                android:background="@drawable/text_border"
                android:textColor="@color/darkBrown"
                android:textSize="@dimen/textSize20dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/homeicon_border"
            android:layout_weight="1"
            android:layout_marginLeft="7dp">
            <ImageView
                android:id="@+id/image_halwa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_omanihalwa"
                android:paddingTop="@dimen/margin40dp"/>
            <TextView
                android:id="@+id/halwa_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/sweets"
                android:gravity="center_horizontal"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
                android:padding="9dp"
                android:background="@drawable/text_border"
                android:textColor="@color/darkBrown"
                android:textSize="@dimen/textSize20dp"/>
        </LinearLayout>
    </LinearLayout>


