<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.herfa.android.herfa.HomeActivity"
    android:orientation="vertical"
    android:background="@drawable/background">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:padding="@dimen/margin15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/homeicon_border"
            android:layout_weight="0.33"
            android:layout_marginRight="7dp">
            <ImageView
                android:id="@+id/image_carpentry"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_carpentry"
                android:paddingTop="@dimen/margin40dp"/>
            <TextView
            android:id="@+id/carpentry_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/carpentry"
            android:gravity="center_horizontal"
            android:layout_marginRight="1dp"
            android:layout_marginLeft="1dp"
            android:padding="9dp"
            android:background="@drawable/text_border"
            android:textColor="@color/darkBrown"
                android:textSize="@dimen/textSize20dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/homeicon_border"
            android:layout_weight="0.33"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="7dp">
            <ImageView
                android:id="@+id/image_blacksmithing"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_blacksmithing"
                android:paddingTop="@dimen/margin40dp"/>
            <TextView
            android:id="@+id/blacksmithing_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/blacksmithing"
            android:gravity="center_horizontal"
            android:layout_marginRight="1dp"
            android:layout_marginLeft="1dp"
            android:padding="9dp"
            android:background="@drawable/text_border"
            android:textColor="@color/darkBrown"
                android:textSize="@dimen/textSize20dp"/>
        </LinearLayout>
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:padding="@dimen/margin15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/homeicon_border"
            android:layout_weight="0.33"
            android:layout_marginRight="7dp">
            <ImageView
                android:id="@+id/image_sewing"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_sewing"
                android:paddingTop="@dimen/margin40dp"/>
            <TextView
                android:id="@+id/sewing_txt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/sewing"
                android:gravity="center_horizontal"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
                android:padding="9dp"
                android:background="@drawable/text_border"
                android:textColor="@color/darkBrown"
                android:textSize="@dimen/textSize20dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/homeicon_border"
            android:layout_weight="0.33"
            android:layout_marginLeft="7dp">
            <ImageView
                android:id="@+id/image_pottery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_ceramics"
                android:paddingTop="@dimen/margin40dp"/>
            <TextView
                android:id="@+id/pottery_txt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/pottery"
                android:gravity="center_horizontal"
                android:layout_marginRight="1dp"
                android:layout_marginLeft="1dp"
                android:padding="9dp"
                android:background="@drawable/text_border"
                android:textColor="@color/darkBrown"
                android:textSize="@dimen/textSize20dp"/>
        </LinearLayout>
    </LinearLayout>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:padding="@dimen/margin15dp">

